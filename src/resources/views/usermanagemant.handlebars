<div class="container-xl">
    <div class="table-responsive">
        <div class="table-wrapper">
            <div class="table-title">
                <div class="row">
                    <div class="col-sm-10" id="titleordermanagement">
                        <h2><b>User</b> Management</h2>
                    </div>
                </div>
            </div>
            <input type="text" name="searchorder" id="searchorder" class="form-control form-control-sm" placeholder="Enter username ...">
            <table class="table table-striped table-hover">
                <thead>
                    <tr>
                        <th>User Name</th>
                        <th>First Name</th>						
                        <th>Last Name</th>
                        <th>Create Course Status</th>
                        <th>Revenue</th>
                        <th>User Detail</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each user}}
                        <tr>
                            <th>{{this.username}}</th>
                            <td>{{this.firstname}}</td>
                            <td>{{this.lastname}}</td>
                            {{#if this.create_course_status}}
                                <td><span class="status text-success">&bull;</span> Enable</td>
                            {{else}}
                                <td><span class="status text-danger">&bull;</span> Disable</td>
                            {{/if}}
                            <td>{{this.revenue}}$</td>
                            <td><a href="/usermanagement/{{this._id}}"><button class="btn btn-outline-secondary" id="btnuserdetail">Detail</button></a></td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>
</div>














