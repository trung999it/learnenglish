<div class="contain-form-login">
	<div class="form-login shadow p-3 mb-5 bg-white rounded" >
		<div class="tittle-login">
			<p>Account Login</p>
		</div>
		<div class="warning-mes">
			<p>We're sorry, there is an error with your email and/or password. Remember passwords are 4 to 12 characters (letters or numbers) long. Please try again. </p>
		</div>
    <form action="/login" method="post">
		<div class="form-input">
			<p>Email Address</p>
			<input type="text" name="emailaddress" id="emailaddress" class="form-control">
			<p>Pass Word</p>
			<input type="password" name="password" id="password" class="form-control">

			<button class="btn btn-outline-primary" id="btnlogin"> Login </button>

			<table class="table-menu-account">
				<tr>
					<td>
						<a href="createaccount">Create Account</a>
					</td>
					<td id="tdright">
						<a href="#">Fogot Password?</a>
					</td>
				</tr>
			</table>
		</div>
    </form>
		<div class="other-method-login">
			<p>Or Login With</p>
		</div>
		<section class="mb-4" id="footer-form">
      <!-- Facebook -->
      <a
        class="btn btn-primary btn-floating m-1"
        style="background-color: #3b5998;"
        href="#!"
        role="button"
        ><i class="fab fa-facebook-f"></i
      ></a>

      <!-- Twitter -->
      <a
        class="btn btn-primary btn-floating m-1"
        style="background-color: #55acee;"
        href="#!"
        role="button"
        ><i class="fab fa-twitter"></i
      ></a>

      <!-- Google -->
      <a
        class="btn btn-primary btn-floating m-1"
        style="background-color: #dd4b39;"
        href="#!"
        role="button"
        ><i class="fab fa-google"></i
      ></a>

      

      <!-- Linkedin -->
      <a
        class="btn btn-primary btn-floating m-1"
        style="background-color: #0082ca;"
        href="#!"
        role="button"
        ><i class="fab fa-linkedin-in"></i
      ></a>
      
    </section>
